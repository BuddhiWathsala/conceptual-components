#################################################################
#    Classes
#################################################################

###  http://myreview#AcceptedPaper
:AcceptedPaper rdf:type owl:Class ;
               rdfs:subClassOf :Paper ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isAcceptedBy ;
                                 owl:someValuesFrom :Chair
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :isRatedBy ;
                                 owl:someValuesFrom :Review
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasStatus ;
                                 owl:hasValue :Accept
                               ] ;
               owl:disjointWith :RejectedPaper .


###  http://myreview#Administrator
:Administrator rdf:type owl:Class ;
               rdfs:subClassOf :ProgramCommitteeMember .


###  http://myreview#Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf :Person .


###  http://myreview#BookletOfAbstracts
:BookletOfAbstracts rdf:type owl:Class ;
                    rdfs:subClassOf :OutputDocument ;
                    owl:disjointWith :CD-ROM ,
                                     :ExternalReviewersList ,
                                     :IndexOfAuthors ,
                                     :ListOfAbstracts ,
                                     :PCMembersList ,
                                     :Proceedings ,
                                     :Program .


###  http://myreview#CD-ROM
:CD-ROM rdf:type owl:Class ;
        rdfs:subClassOf :OutputDocument ;
        owl:disjointWith :ExternalReviewersList ,
                         :IndexOfAuthors ,
                         :ListOfAbstracts ,
                         :PCMembersList ,
                         :Proceedings ,
                         :Program .


###  http://myreview#Chair
:Chair rdf:type owl:Class ;
       rdfs:subClassOf :ProgramCommitteeMember .


###  http://myreview#Conference
:Conference rdf:type owl:Class ;
            rdfs:subClassOf owl:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasAcronym ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasEmail ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :hasName ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            owl:disjointWith :ConferenceProgram ,
                             :Criterion ,
                             :Document ,
                             :Fee ,
                             :OutputDocument ,
                             :PaperStatus ,
                             :PaymentMode ,
                             :Person ,
                             :ProgramCommittee ,
                             :Question ,
                             :ResearchTopic ,
                             :Session ,
                             :Slot .


###  http://myreview#ConferenceAttendant
:ConferenceAttendant rdf:type owl:Class ;
                     rdfs:subClassOf :Person ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :mustPay ;
                                       owl:someValuesFrom :Fee
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :paid ;
                                       owl:hasValue "true"^^xsd:boolean
                                     ] ,
