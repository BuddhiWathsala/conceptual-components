@prefix ns1: <https://www.ica.org/standards/RiC/ontology#> .
@prefix ns2: <https://w3id.org/framester/schema/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ns1:Agent ns2:subsumedUnder ns1:Thing .

ns1:Person ns2:subsumedUnder ns1:Agent ;
    ns1:deathDate rdfs:Literal ;
    ns1:hasAncestor ns1:Person ;
    ns1:hasChild ns1:Person ;
    ns1:hasDescendant ns1:Person ;
    ns1:hasFamilyAssociationWith ns1:Person ;
    ns1:hasOrHadCorrespondent ns1:Person ;
    ns1:hasOrHadSpouse ns1:Person ;
    ns1:hasOrHadStudent ns1:Person ;
    ns1:hasOrHadTeacher ns1:Person ;
    ns1:hasSibling ns1:Person ;
    ns1:isChildOf ns1:Person ;
    ns1:knownBy ns1:Person ;
    ns1:knows ns1:Person ;
    ns1:knowsOf ns1:Person .

