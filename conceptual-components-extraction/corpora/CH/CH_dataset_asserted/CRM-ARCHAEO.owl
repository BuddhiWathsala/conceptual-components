    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/CRMsci/S11_Amount_of_Matter"/>
    <rdfs:label xml:lang="en">discarded into</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/AP3_excavated">
    <rdfs:label xml:lang="en">excavated</rdfs:label>
    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/A1_Excavation_Process_Unit"/>
    <rdfs:comment xml:lang="en">This property identifies the 3D excavated volume instance of E53 Place, i.e., a three- dimensional volume, that was actually excavated during an A1 Excavation Process Unit. </rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/AP13i_is_stratigraphic_relation_of">
