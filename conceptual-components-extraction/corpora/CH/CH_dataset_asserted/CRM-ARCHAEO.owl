    <rdfs:label xml:lang="en">provided matter to</rdfs:label>
    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/A4_Stratigraphic_Genesis"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/CRMsci/S10_Material_Substantial"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/AP2_discarded_into">
