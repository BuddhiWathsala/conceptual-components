  <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/AP15_is_or_contains_remains_of">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">is or contains remains of</rdfs:label>
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/A8_Stratigraphic_Unit"/>
    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E18_Physical_Thing"/>
    <rdfs:comment xml:lang="en">This property associates an E18 Physical Thing that is found within an A8 Stratigraphic  Unit with the stratigraphic unit. This property is a shortcut for the fully articulated path from E18 Physical Thing through A7 Embedding to A8 Stratigraphic Unit.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/AP20i_contains">
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    <rdfs:range rdf:resource="http://www.cidoc-crm.org/cidoc-crm/CRMarchaeo/A7_Embedding"/>
