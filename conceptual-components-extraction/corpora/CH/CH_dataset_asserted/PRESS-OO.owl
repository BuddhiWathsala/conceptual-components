  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Y11i_was_separated_through">
    <rdfs:range rdf:resource="http://iflastandards.info/ns/fr/frbr/pressoo/Z3_Separation"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">was separated through</rdfs:label>
    <rdfs:subPropertyOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/R16i_was_initiated_by"/>
    <rdfs:domain rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F18_Serial_Work"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Z4_Temporary_Substitution">
    <rdfs:label xml:lang="en">Temporary Substitution</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F30_Publication_Event"/>
    <rdfs:comment xml:lang="en">replacement for another instance of F18 Serial Work. The replacing instance of F18 Serial Work can either pre-exist the replacement, or be created specifically for the purpose of replacing the other instance of F18 Serial Work.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Y43i_is_exemplified_by">
    <rdfs:domain rdf:resource="http://iflastandards.info/ns/fr/frbr/pressoo/Z10_Sequencing_Pattern"/>
