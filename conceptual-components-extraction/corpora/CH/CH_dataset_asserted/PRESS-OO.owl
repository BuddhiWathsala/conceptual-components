  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Y37_has_former_or_current_issuing_rule">
    <rdfs:range rdf:resource="http://iflastandards.info/ns/fr/frbr/pressoo/Z12_Issuing_Rule"/>
