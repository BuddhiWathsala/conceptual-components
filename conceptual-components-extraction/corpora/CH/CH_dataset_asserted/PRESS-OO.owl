    <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P2i_is_type_of"/>
    <rdfs:range rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F23_Expression_Fragment"/>
    <rdfs:label xml:lang="en">is exemplified by</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Z2_Absorption">
    <rdfs:comment xml:lang="en">This class comprises activities that result in instances of F18 Serial Work being absorbed by pre-existing instances of F18 Serial Work.
An absorption can be complete or only partial (e.g., a serial devoted to history in general can absorb specifically the portion devoted to the history of the Middle Ages of another serial that is devoted to the Middle Ages and Renaissance, while the portion devoted to the Renaissance is just abandoned). This information element can be reflected through the P2 has type (is type of): E55 Type from the CIDOC CRM model.</rdfs:comment>
    <rdfs:label xml:lang="en">Absorption</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E7_Activity"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Y35_was_absorbed_in">
    <rdfs:domain rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F18_Serial_Work"/>
    <rdfs:comment xml:lang="en">This property associates instances of F18 Serial Work with the instances of F18 Serial Work that absorbed them.</rdfs:comment>
    <rdfs:range rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F18_Serial_Work"/>
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdfs:label xml:lang="en">was absorbed in</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://iflastandards.info/ns/fr/frbr/pressoo/Y43_is_indicative_of">
    <rdfs:domain rdf:resource="http://iflastandards.info/ns/fr/frbr/frbroo/F23_Expression_Fragment"/>
