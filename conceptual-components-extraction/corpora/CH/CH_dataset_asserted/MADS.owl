    <!-- http://www.loc.gov/mads/rdf/v1#Affiliation -->

    <owl:Class rdf:about="http://www.loc.gov/mads/rdf/v1#Affiliation">
        <rdfs:comment xml:lang="en">A resource that describes an individual&apos;s affiliation with an organization or group, such as the nature of the affiliation and the active dates.</rdfs:comment>
