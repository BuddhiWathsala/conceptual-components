a-mp:MetallicSpecimen a owl:Class ;
    rdfs:label "Metallic specimen"@en,
        "Campione metallico"@it ;
    rdfs:comment "This class represents the metallic specimen"@en,
        "Questa classe rappresenta il campione metallico"@it ;
    rdfs:isDefinedBy <https://w3di.org/arco/ontology/movable-property/> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Reflectance ;
            owl:onProperty a-mp:hasReflectance ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:boolean ;
            owl:onProperty a-mp:internalReflection ],
        a-mp:Specimen ;
    owl:disjointWith a-mp:NotMetallicSpecimen ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "ICCD Normativa BNM: SM/SMM" .

