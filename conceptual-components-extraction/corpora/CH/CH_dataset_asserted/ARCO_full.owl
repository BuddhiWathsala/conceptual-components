            owl:onProperty default3:hasAgentWithResponsibility ;
            owl:someValuesFrom l0:Agent ],
        [ a owl:Restriction ;
            owl:onProperty default3:isResponsibilityFor ;
            owl:someValuesFrom cis:CulturalEntity ],
        [ a owl:Restriction ;
            owl:onProperty default3:hasResponsibilityType ;
            owl:someValuesFrom default3:ResponsibilityType ],
