a-mp:Chron a owl:Class ;
    rdfs:subClassOf a-mp:GeochronologicalClass .

a-mp:ChronostratigraphicSeries a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:ChronostratigraphicStage ;
            owl:onProperty a-mp:hasDirectLowerRank ],
        a-mp:ChronostratigraphicClass .

a-mp:ChronostratigraphicStage a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:ChronostratigraphicSubstage ;
            owl:onProperty a-mp:hasDirectLowerRank ],
        a-mp:ChronostratigraphicClass .

a-mp:ChronostratigraphicSubstage a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Chronozone ;
            owl:onProperty a-mp:hasDirectLowerRank ],
        a-mp:ChronostratigraphicClass .

a-mp:ChronostratigraphicSystem a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:ChronostratigraphicSeries ;
            owl:onProperty a-mp:hasDirectLowerRank ],
        a-mp:ChronostratigraphicClass .

a-mp:Chronozone a owl:Class ;
    rdfs:subClassOf a-mp:ChronostratigraphicClass .

a-mp:Erathem a owl:Class ;
