    rdfs:range l0:Agent ;
    rdfs:subPropertyOf default3:hasRelatedAgent ;
    owl:inverseOf default3:isProponentAgencyOf ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa Trasversale 4.00: TU/NVC/NVCA; Versioni precedenti: CD/EPR" .

