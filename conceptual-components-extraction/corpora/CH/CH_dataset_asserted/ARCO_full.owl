    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa A: CP" .

a-ip:RoofStructure a owl:Class ;
    rdfs:label "Roof structure"@en,
        "Struttura del tetto"@it ;
    rdfs:comment "This class represents the structural solution of the roof of a construction."@en,
        "Questa classe rappresenta la soluzione strutturale della copertura di una costruzione."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf l0:Object ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

a-ip:Stair a owl:Class ;
    rdfs:label "Stair"@en,
        "Scala"@it ;
    rdfs:comment "This class represents a stair."@en,
        "Questa classe rappresenta una scala."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:positiveInteger ;
            owl:onProperty a-ip:numberOfRises ],
        a-ip:VerticalConnection ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

a-ip:WaterAvailability a owl:Class ;
    rdfs:label "Water availability"@en,
        "Disponibilità idrica"@it ;
    rdfs:comment "This class represents the water availability of a water system."@en,
        "Questa classe rappresenta la disponibilità idrica di un impianto idrico."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf default4:CulturalEntityCharacteristic ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa PG: CA/CAI" .

a-ip:WaterDistributionType a owl:Class ;
    rdfs:label "Water distribution type"@en,
        "Tipo di distribuzione idrica"@it ;
    rdfs:comment "This class represents the water distribution type of a water system."@en,
        "Questa classe rappresenta il tipo di distribuzione idrica di un impianto idrico."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf default4:CulturalEntityCharacteristic ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa PG: CA/CAI" .

a-ip:WaterElement a owl:Class ;
    rdfs:label "Water element"@en,
