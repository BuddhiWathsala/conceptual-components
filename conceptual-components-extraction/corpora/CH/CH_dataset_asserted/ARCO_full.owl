default4:TimeIndexedSituation a owl:Class ;
    rdfs:label "Time indexed situation"@en,
        "Situazione nel tempo"@it ;
    opla11:isNativeTo <http://ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl> ;
    rdfs:comment "The class representing the generic concept of time indexed situation."@en,
        "La classe che rappresenta il generico concetto di situazione indicizzata nel tempo."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
    rdfs:subClassOf default4:Situation ;
    owl:versionInfo "stable"@en,
        "stabile"@it .

default4:isAgentInvolvedIn a owl:ObjectProperty ;
    rdfs:label "is agent involved in"@en,
        "è agente coinvolto in"@it ;
    rdfs:comment "This property represents an agent partecipation"@en,
        "Questa proprietà rappresenta la partecipazione di un agente."@it ;
    rdfs:domain l0:Agent ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
    rdfs:range l0:EventOrSituation ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

default4:isCategoryOf a owl:ObjectProperty ;
    rdfs:label "is category of"@en,
        "è categoria di"@it ;
    rdfs:comment "The object property that associates a category to any object."@en,
        "La object property che associa una categoria a un qualunque oggetto."@it ;
    rdfs:domain owl:Thing ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
    rdfs:range default4:Category ;
    rdfs:subPropertyOf default4:isConceptOf ;
    owl:versionInfo "stable"@en,
        "stabile"@it .

default4:note a owl:DatatypeProperty ;
    rdfs:label "note"@en,
        "note"@it ;
    rdfs:comment "This property represents notes associated to a piece of information."@en,
        "Questa proprietà rappresenta eventuali note, relative a una certa informazione."@it ;
    rdfs:domain owl:Thing ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
    rdfs:range rdfs:Literal ;
    owl:versionInfo "stable"@en,
        "stabile"@it .

default4:specifications a owl:DatatypeProperty ;
    rdfs:label "specifications"@en,
        "specifiche"@it ;
    rdfs:comment "This property represents specifications associated to a piece of information."@en,
        "Questa proprietà rappresenta eventuali specifiche, relative a una certa informazione."@it ;
    rdfs:domain owl:Thing ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/core> ;
    rdfs:range rdfs:Literal ;
    owl:versionInfo "stable"@en,
        "stabile"@it .

default6:CulturalPropertyAvailability a owl:Class ;
    rdfs:label "Cultural Property Availability"@en,
        "Disponibilità materiale del bene culturale"@it ;
    rdfs:comment "This class represents the availability of a cultural property, wheter the cultural property is available or not (e.g.: theft, destruction) for example for cataloguing activities."@en,
        "Questa classe rappresenta la disponibilità materiale di un bene culturale, ovvero se il bene è materialmente disponibile, per esempio per attività di catalogazione, o se non è disponibile (es.: furto, distruzione)"@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/denotative-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default6:isCulturalPropertyAvailabilityOf ;
            owl:someValuesFrom cis:CulturalEntity ],
        default4:CulturalEntityCharacteristic ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa Trasversale 4.00: OG/OGR" .

default6:FunctionalPurpose a owl:Class ;
    rdfs:label "Functional purpose"@en,
        "Scopo funzionale"@it ;
    rdfs:comment "This class represents the functional purpose (i.e. decorative, religious, symbolic, magic-offensive) of an object (i.e. culural property or iconographic/decorative apparatus or affixed element on a cultural property like an inscription). The functional purpose is intended as the function that an object has to perform."@en,
        "Questa classe rappresenta lo scopo funzionale (decorativo, religioso, simbolico, magico-offensivo, etc.), inteso come funzione che deve svolgere, di un oggetto (es bene culturale, apparato iconografico/decorativo o elemento apposto su un bene culturale come un'iscrizione)."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/denotative-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default6:isFunctionalPurposeOf ;
            owl:someValuesFrom owl:Thing ],
        default4:CulturalEntityCharacteristic ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa Trasversale: DA/ISE/ISEC - Normative di versioni precedenti: DA/ISR/ISRC; DA/APF/APFF - Normative MA e CA: OG/OGT/OGTF"@it .

default6:IconographicOrDecorativeApparatusType a owl:Class ;
    rdfs:label "Iconographic Or Decorative Apparatus Type"@en,
        "Tipo di apparato iconografico e decorativo"@it ;
    rdfs:comment "This class represents the iconographic or decorative apparatus' type (iconographic, decorative, iconographic-decorative) of a cultual property."@en,
        "Questa classe rappresenta il tipo (iconografico, decorativo, iconografico-decorativo) di apparato iconografico e decorativo di un bene culturale."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/denotative-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default6:isIconographicOrDecorativeApparatusTypeOf ;
            owl:someValuesFrom default6:IconographicOrDecorativeApparatus ],
        default4:Type ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa Trasversale 4.00: DA/AID/AIDO" .

a-ip:ConstructionElement a owl:Class ;
    rdfs:label "Construction element"@en,
        "Elemento della costruzione"@it ;
    rdfs:comment "This class represents all the physical elements that are part of a construction."@en,
        "Questa classe rappresenta tutti gli elementi fisici che fanno parte di una costruzione."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf a-ip:Construction ;
    owl:disjointWith a-ip:ConstructionSpace ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

a-ip:Layout a owl:Class ;
    rdfs:label "Layout"@en,
        "Schema iconografico"@it ;
    rdfs:comment "This class represents the shape of the plan and the elevation of a construction and the way in which the different parts of them are arranged in the whole composition."@en,
        "Questa classe rappresenta la forma della pianta e del prospetto di una costruzione e il modo in cui le varie parti di essi sono assemblate nella composizione d'insieme."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf l0:Description ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa A: PN/PNT/PNTS e PN/PNT/PNTF" .

a-ip:VerticalElement a owl:Class ;
    rdfs:label "Vertical element"@en,
        "Elemento verticale"@it ;
    rdfs:comment "This class represents the architectural vertical elements, both load-bearing and not load-bearing, that bound and subdivide the spaces of a construction."@en,
        "Questa classe rappresenta gli elementi architettonici verticali, sia strutturali che non strutturali, che delimitano e suddividono gli spazi della costruzione. "@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-ip:VerticalElementType ;
            owl:onProperty default4:hasType ],
        a-ip:ArchitecturalElement ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa A: SV" .

default8:OldTownAdmistrativeType a owl:Class ;
    rdfs:label "Old town administrative type"@en,
        "Tipo di carattere amministrativo del centro storico"@it ;
    rdfs:comment "This class represents the information clarifying the administrative character of the old town, which may be: a chief town, a town fraction, an incorporated area or a locality."@en,
        "Questa classe rappresenta le informazioni che precisano il carattere amministrativo del centro storico, che può essere: un capoluogo, una frazione, un centro inglobato o una località."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/location> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default8:isOldTownAdministrativeTypeOf ;
            owl:someValuesFrom default8:OldTown ],
        default4:Type ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normative A e PG: LC/CST/CSTA"@it .

a-mp:BotanicalForm a owl:Class ;
    rdfs:label "Botanical form"@en,
        "Forma botanica"@it ;
    rdfs:comment "This class represents a variety from an aesthetic or chromatic point of view."@en,
        "Questa classe rappresenta una varietà dal punto di vista della forma estetica o cromatica"@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/movable-property> ;
    rdfs:subClassOf a-mp:TaxonSpecification ;
    owl:disjointWith a-mp:Cultivar ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "ICCD Normativa BNB: RB/RBN/RBNH; SB/NAA/NAAH; SB/NAT/NATH" .

a-mp:ChronostratigraphicClass a owl:Class ;
    rdfs:subClassOf default4:Concept .

a-mp:CrystalMorphology a owl:Class ;
    rdfs:label "Crystal morphology"@en,
        "Morfologia del cristallo"@it ;
    rdfs:comment "This class represents the morphology of a crystal."@en,
        "Questa classe rappresenta lamorfologia di un cristallo."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/movable-property> ;
    rdfs:subClassOf l0:Description ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "ICCD Normativa BNM: SM/SMA/SMAB" .

a-mp:GeochronologicalClass a owl:Class ;
    rdfs:subClassOf default4:Concept .

a-mp:GeologicalContext a owl:Class ;
    rdfs:label "Geological context"@en,
        "Contesto geologico"@it ;
    rdfs:comment "This class represents geological context of a collecting location."@en,
        "Questa classe rappresenta il contesto geologico del luogo di raccolta di un bene."@it ;
    rdfs:isDefinedBy <https://w3di.org/arco/ontology/movable-property/> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Outcrop ;
            owl:onProperty a-mp:hasOutcrop ],
        [ a owl:Restriction ;
            owl:allValuesFrom a-mp:MineralDeposit ;
            owl:onProperty a-mp:hasMineralDeposit ],
        default4:Situation ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "ICCD Normativa BNM: IM/IMA/IMAC" .

a-mp:OpticCharacteristic a owl:Class ;
    rdfs:label "Optic characteristic"@en,
        "Caratteristica ottica"@it ;
    rdfs:comment "This class represents the optic characteristic of a mineral."@en,
        "Questa classe rappresenta la caratteristica ottica di un minerale."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/movable-property> ;
    rdfs:subClassOf a-mp:PhysicalCharacteristic ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

a-mp:Phylum a owl:Class ;
    rdfs:label "Phylum"@en,
        "Phylum"@it ;
    rdfs:comment "This class represents one level of a biological taxon, the phylum"@en,
        "Questa classe rappresenta uno dei livelli di classificazione di una tassonomia biologica, ovvero il phylum."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/movable-property> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Kingdom ;
            owl:onProperty a-mp:hasDirectHigherRank ],
        [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Class ;
            owl:onProperty a-mp:hasDirectLowerRank ],
        [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Kingdom ;
            owl:onProperty a-mp:hasHigherRank ],
        [ a owl:Restriction ;
            owl:allValuesFrom [ a owl:Class ;
                    owl:complementOf [ a owl:Class ;
                            owl:unionOf ( a-mp:Kingdom a-mp:Phylum ) ] ] ;
            owl:onProperty a-mp:hasLowerRank ],
        a-mp:BiologicalTaxon ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

a-mp:Substrate a owl:Class ;
    rdfs:label "Substrate"@en,
        "Substrato"@it ;
    rdfs:comment "This class represents  the specific substrate where the specimen was collected"@en,
        "Questa classe rappresenta substrato specifico sul quale è stato raccolto il campione"@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/movable-property> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom a-mp:SubstrateType ;
            owl:onProperty default4:hasType ],
        [ a owl:Restriction ;
            owl:allValuesFrom a-mp:Incline ;
            owl:onProperty a-mp:hasIncline ],
        l0:Object ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "ICCD Normativa BNB: LR/LRI/LRIR" .

a-mp:hasHigherRank a owl:ObjectProperty ;
    rdfs:label "has higher rank"@en,
        "ha rango superiore"@it ;
    rdfs:comment "This property relates two taxa, where the first is more specific than the second."@en,
        "Questa proprietà collega due taxa, dove la prima è più specifica della seconda."@it ;
    rdfs:domain a-mp:Taxon ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/movable-property> ;
    rdfs:range a-mp:Taxon ;
    owl:inverseOf a-mp:hasLowerRank ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

tiapit:time a owl:DatatypeProperty .

:PhotographicHeritageClassification a owl:Class ;
    rdfs:label "Photographic heritage classification"@en,
        "Classificazione di bene fotografico"@it ;
    rdfs:comment "This class represents a photographic entity's classification accorting to other classification systems as Iconclass. The system has to be recognised and used at national or preferably international level."@en,
        "Questa classe rappresenta una classificazione di un bene fotografico secondo un sistema di classificazione alternativo a Iconclass, che sia comunque riconosciuto e utilizzato a livello nazionale, o preferibilmente internazionale."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/arco> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasPhotographicHeritageClassificationType ;
            owl:someValuesFrom :PhotographicHeritageClassificationType ],
        [ a owl:Restriction ;
            owl:onProperty :isPhotographicHeritageClassificationOf ;
            owl:someValuesFrom :PhotographicHeritage ],
        [ a owl:Restriction ;
            owl:onProperty :classifiedSubject ;
            owl:someValuesFrom rdfs:Literal ],
        default4:CulturalEntityClassification ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa F 4.00: SG/CLF" .

default1:AdditionalForm a owl:Class ;
    rdfs:label "Additional form"@en,
        "Modulo di approfondimento"@it ;
    rdfs:comment "This class represents an additional form, that describes specific information about a cultural property (epigraphic documents, stratigraphy, unit, wall typology)"@en,
        "Questa classe rappresenta un modulo di approfondimento, utile a trattare secondo un approccio specialistico specifici aspetti del bene in esame (Documenti Epigrafici; Tipi Murari, Unità Stratigrafiche)"@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/catalogue> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default1:describes ;
            owl:someValuesFrom cis:CulturalEntity ],
        [ a owl:Restriction ;
            owl:onProperty default1:additionalFormIdentifier ;
            owl:someValuesFrom rdfs:Literal ],
        [ a owl:Restriction ;
            owl:onProperty default1:hasAdditionalFormType ;
            owl:someValuesFrom default1:AdditionalFormType ],
        l0:Object ;
    owl:disjointWith default1:InformationForm ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa Trasversale 4.00: DA/APP" .

default3:CollectionMembership a owl:Class ;
    rdfs:label "Collection Membership"@en,
        "Appartenenza a Collezione"@it ;
    rdfs:comment "This class represents the membership of a cultural property in a collection."@en,
        "Questa classe rappresenta l'appartenenza di un bene culturale a una collezione, in un certo intervallo di tempo. Uno stesso bene culturale può aver fatto parte di più collezioni nel corso del tempo."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/context-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom rdfs:Literal ;
            owl:onProperty default3:collectionUnitIdentifier ],
        [ a owl:Restriction ;
            owl:allValuesFrom cis:CollectionCulEnt ;
            owl:onProperty default3:hasCollection ],
        [ a owl:Restriction ;
            owl:allValuesFrom rdfs:Literal ;
            owl:onProperty default3:collectionLeavingReason ],
        [ a owl:Restriction ;
            owl:onProperty default3:hasMemberOfCollection ;
            owl:someValuesFrom cis:CulturalEntity ],
        [ a owl:Restriction ;
            owl:allValuesFrom rdfs:Literal ;
            owl:onProperty default3:estimatedCulturalPropertyValue ],
        [ a owl:Restriction ;
            owl:allValuesFrom tiapit:TimeInterval ;
            owl:onProperty tiapit:atTime ],
        default3:Membership ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "UB/COL" .

default3:Dating a owl:Class ;
    rdfs:label "Dating"@en,
        "Cronologia"@it ;
    rdfs:comment "This class represents the dating of an entity, that is information about the chronology of the entity (e.g.: a cultural property), referring to a particular event (e.g.: the creation of the cultural property, its distruction, etc.)"@en,
        "Questa classe rappresenta la cronologia di un'entità, cioè i dati cronologici che riguardano la storia dell'entità (es.: un bene culturale o un'opera collegata a un bene culturale). In relazione alle diverse situazioni, indicheranno un periodo esteso nel tempo oppure una singola data. Per ciascun bene viene fornita la cronologia della sua realizzazione, o, nel caso di immobili che presentano diverse fasi edilizie o diversi momenti di vita, un arco cronologico di riferimento; possono essere fornite anche altre notizie cronologiche relative ad altri eventi che hanno riguardato il bene (demolizione, progettazione, rimaneggiamenti, etc.)"@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/context-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default3:isDatingOf ;
            owl:someValuesFrom cis:CulturalEntity ],
        [ a owl:Restriction ;
            owl:allValuesFrom l0:EventOrSituation ;
            owl:onProperty default3:hasDatingEvent ],
        default3:DetectionMethod,
        default3:Interpretation ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa Trasversale 4.00: DT - ICCD Normative F, RA, SMO, PST, S, OAC, OA, MI, D, NU: RO/ROF/ROFD" .

default3:DerivatedWork a owl:Class ;
    rdfs:label "Derivated work"@en,
        "Opera derivata"@it ;
    rdfs:comment "This class represents a derivated work of a cultural property (copy, reuse, facsimile, forgery), created by the same author or by another author."@en,
        "Questa classe rappresenta una derivazione del bene catalogato (reimpiego, copia, contraffazione, replica, controtipo) prodotta dallo stesso o da altro autore."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/context-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom l0:Agent ;
            owl:onProperty default3:hasAuthor ],
        [ a owl:Restriction ;
            owl:allValuesFrom rdfs:Literal ;
            owl:onProperty default3:derivatedWorkLocation ],
        [ a owl:Restriction ;
            owl:allValuesFrom default3:DerivatedWorkType ;
            owl:onProperty default3:hasDerivatedWorkType ],
        cis:CulturalEntity ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa S 3: RO/COP - Normativa F: RO/CRF - Normative RA, SMO, PST, OAC, OA, MI, D, NU: RO/REI - Normative CA, MA: RO/RIU" .

default3:Interpretation a owl:Class ;
    rdfs:label "Interpretation"@en,
        "Interpretazione"@it ;
    dc:description """An example of usage of one of the subclasses of the class 'Interpretation' is the following:
resource:PreferredAuthorshipAttribution/1200865600-1 a           cd:PreferredAuthorshipAttribution ;
        rdfs:label  "Preferred authorship attribution of cultural property: 1200865600"@en ;
        cd:hasAttributedAuthor resource:Agent/cavaceppi-bartolomeo-1717-1799 ;
        cd:hasSource resource:Source/documentation ."""@en,
        """Un esempio di utilizzo di una delle sottoclassi della classe "Interpretazione" è il seguente:
resource:PreferredAuthorshipAttribution/1200865600-1 a           cd:PreferredAuthorshipAttribution ;
        rdfs:label "Attribuzione di autore preferita, maggiormente accreditata o convincente del bene: 1200865600"@it ;
        cd:hasAttributedAuthor resource:Agent/cavaceppi-bartolomeo-1717-1799 ;
        cd:hasSource resource:Source/documentazione ."""@it ;
    rdfs:comment "This class represents a situation in which pieces of information related to an object (e.g.: a cultural property) are detected and recorded by an agent, based on a specific source or motivation (e.g.:ithe authorship attribution, commission, etc.)"@en,
        "Questa classe rappresenta una situazione in cui viene individuata e registrata da un agente una certa informazione o un gruppo di informazioni relativamente a un oggetto, per esempio a un bene culturale o a una sua parte, supportata da una fonte o motivazione. Per esempio, l'attribuzione di autore, l'individuazione della committenza o delle entità responsabili di fasi successive alla creazione di un bene culturale sono tutte interpretazioni basate su fonti."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/context-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty default3:hasInterpretationCriterion ;
            owl:someValuesFrom default3:InterpretationCriterion ],
        default4:Situation ;
    owl:versionInfo "stable"@en,
        "stabile"@it .

default3:Intervention a owl:Class ;
    rdfs:label "Intervention"@en,
        "Intervento"@it ;
    dc:description """An example of usage of the class 'Intervention' is the following:
resource:Intervention/0800640527-10-intervention-1
        a           cd:Intervention ;
        rdfs:label  "Intervention 1 on cultural property 0800640527-10"@en ;
        cd:hasActivityOperator resource:Agent/tonelli-elvira ;
        cd:hasActivityResponsible resource:Agent/soprintendenza-per-i-beni-ambientali-ed-architettonici-di-bologna-modena-e-reggio-emilia ;
        cd:hasInterventionType resource:InterventionType/restauro ;
        tiapit:atTime resource:TimeInterval/1994 ."""@en,
        """Un esempio di utilizzo della classe "Intervento" è il seguente:
resource:Intervention/0800640527-10-intervention-1
        a           cd:Intervention ;
        rdfs:label  "Intervento 1 sul bene culturale 0800640527-10"@it ;
        cd:hasActivityOperator resource:Agent/tonelli-elvira ;
        cd:hasActivityResponsible resource:Agent/soprintendenza-per-i-beni-ambientali-ed-architettonici-di-bologna-modena-e-reggio-emilia ;
        cd:hasInterventionType resource:InterventionType/restauro ;
        tiapit:atTime resource:TimeInterval/1994 ."""@it ;
    rdfs:comment "This class represents an intervention on a cultural property, that is an activity carried out for its conservation or for other purposes: conservation interventions, reprocessing, restoring, etc."@en,
        "Questa classe rappresenta un intervento che ha interessato un bene culturale, a fini conservativi (per i quali si crea una sottoclasse di \"Intervento\") oppure di altro tipo: restauri (termine obsoleto che attualmente corrisponde a \"intervento a fine conservativo\"), rilavorazioni, ripristini, trasformazioni per recuperare aspetto e funzionalità rispondenti ai mutati gusti, ecc. In generale, per interventi si intendono attività che riguardano il bene, effettuate per la sua realizzazione/modifica e conservazione materiale."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/context-description> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom cis:CulturalEntity ;
            owl:onProperty default3:isInterventionOn ],
        [ a owl:Restriction ;
