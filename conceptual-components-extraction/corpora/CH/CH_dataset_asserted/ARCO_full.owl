            owl:onProperty :hasCulturalPropertyComponent ],
        [ a owl:Restriction ;
            owl:allValuesFrom :CulturalPropertyResidual ;
            owl:onProperty :hasCulturalPropertyResidual ],
        [ a owl:Restriction ;
            owl:allValuesFrom default6:CulturalPropertyAccessibility ;
            owl:onProperty default6:hasCulturalPropertyAccessibility ],
        [ a owl:Restriction ;
            owl:allValuesFrom default3:LegalSituation ;
            owl:onProperty default3:hasLegalSituation ],
        [ a owl:Restriction ;
            owl:allValuesFrom default3:ExportImportCertification ;
            owl:onProperty default3:hasExportImportCertification ],
