            owl:onProperty :numberOfResiduals ],
        :CulturalProperty ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "OG/OGT/OGTW" .

:HornbostelSachsClassification a owl:Class ;
    rdfs:label "Hornbostel-Sachs classification"@en,
        "Classificazione Hornbostel-Sachs"@it ;
    rdfs:comment "This class represents the Hornbostel-Sachs classification of a musical instrument."@en,
        "Questa classe rappresenta la classificazione Hornbostel-Sachs di uno strumento musicale."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/arco> ;
    rdfs:subClassOf [ a owl:Restriction ;
