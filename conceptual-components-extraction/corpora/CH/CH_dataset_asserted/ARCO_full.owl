        [ a owl:Restriction ;
            owl:allValuesFrom default6:IconographicOrDecorativeApparatus ;
            owl:onProperty default6:hasIconographicOrDecorativeApparatus ],
        [ a owl:Restriction ;
            owl:allValuesFrom default3:Title ;
            owl:onProperty default3:hasTitle ],
        [ a owl:Restriction ;
            owl:allValuesFrom default3:DetectionMethod ;
            owl:onProperty default3:hasDetectionMethod ],
        [ a owl:Restriction ;
