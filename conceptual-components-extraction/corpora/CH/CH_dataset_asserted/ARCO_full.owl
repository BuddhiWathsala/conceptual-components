        [ a owl:Restriction ;
            owl:onProperty default3:hasUser ;
            owl:someValuesFrom l0:Agent ],
        default3:Interpretation ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "Normativa VeAC: AU/FRU" .

