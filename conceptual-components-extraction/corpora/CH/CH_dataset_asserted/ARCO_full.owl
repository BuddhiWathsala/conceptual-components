        [ a owl:Restriction ;
            owl:allValuesFrom roapit:TimeIndexedRole ;
            owl:onProperty default1:hasCatalogueRecordVersionRiT ],
        [ a owl:Restriction ;
            owl:allValuesFrom default1:CatalogueRecordVersion ;
