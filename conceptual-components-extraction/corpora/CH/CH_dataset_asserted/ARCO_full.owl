    rdfs:range :NumismaticProperty ;
    rdfs:subPropertyOf default4:isCategoryOf ;
    owl:versionInfo "stable"@en,
        "stabile"@it ;
    default1:iccdNormTag "ICCD Normativa NU 4.00: OG/OGT/OGTH" .

