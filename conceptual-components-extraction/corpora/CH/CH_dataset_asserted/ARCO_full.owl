    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa A: FN" .

a-ip:FurnishingElement a owl:Class ;
    rdfs:label "Furnishing element"@en,
        "Elemento di mobilio"@it ;
    rdfs:comment "This class represents the elements, both movable or built-in, that are part of the furniture of a construction."@en,
        "Questa classe rappresenta gli elementi, sia mobili sia fissi, che compongono l'arredo di una costruzione."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf a-ip:ConstructionElement ;
    owl:versionInfo "unstable"@en,
        "instabile"@it .

a-ip:ModeOfIrrigation a owl:Class ;
    rdfs:label "Mode of irrigation"@en,
        "Modalità di irrigazione"@it ;
    rdfs:comment "This class represents the modof irrigation of a water system"@en,
        "Questa classe rappresenta la la modalità di irrigazione di un sistema idrico."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf default4:CulturalEntityCharacteristic ;
    owl:versionInfo "unstable"@en,
        "instabile"@it ;
    default1:iccdNormTag "Normativa PG: CA/CAI" .

a-ip:Roof a owl:Class ;
    rdfs:label "Roof"@en,
        "Copertura"@it ;
    rdfs:comment "This class represents a roof."@en,
        "Questa classe rappresenta una copertura."@it ;
    rdfs:isDefinedBy <https://w3id.org/arco/ontology/immovable-property> ;
    rdfs:subClassOf [ a owl:Restriction ;
